'''''
# skriv input talet positiv med condition
while True:
    x = int(input("skriv ett tal: "))
    if x > 0:
        print ("Talet är positiv")
    elif x < 0:
        print("Talet är negativ")
    else:
        print("Talet är noll")
    fortsatt = input("vill du forsätt? (j/n)")
    if fortsatt == "n":
       print ("Hej då!")
       break
