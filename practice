produkt = {
    "name": "Laptop",
    "pris": 1000,
    "lager": 50
}
#print pris
print (produkt["pris"])

#lägg till en ny kategori
produkt ["kategori"] = "elektronics"
print (produkt)

#uppdatera lager
produkt["lager"] = 40
print(produkt)
